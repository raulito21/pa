#include <stdio.h>
# include <stdbool.h>

bool prim(int a)
{ if (a < 2) return false;
for(int i=2;i<=a/2;i++)
    { if (a % i == 0)
        return false;
    }
return true;
}
void generare(int n)
{ printf("numerele prime mai mici ca %d sunt:\n", n);
    for(int i=2;i<n;i++)
    { if (prim(i))
        printf("%d ", i);
    }
    printf("\n");
}
int main()
{ int n;
printf("introduceti un numar natural\n");
scanf("%d", &n);
if( n>=2)
    { generare(n); }
else
    { printf("nu exista numere prime mai mici ca %d", n); }
}
